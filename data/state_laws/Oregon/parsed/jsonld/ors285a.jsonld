                      "label": "(ii)",
                      "token": "ii",
                      "kind": "roman_lower",
                      "text": "Traded sector business development;",
                      "subsections": []
                    },
                    {
                      "label": "(iii)",
                      "token": "iii",
                      "kind": "roman_lower",
                      "text": "Small business development;",
                      "subsections": []
                    },
                    {
                      "label": "(iv)",
                      "token": "iv",
                      "kind": "roman_lower",
                      "text": "Local economic development;",
                      "subsections": []
                    },
                    {
                      "label": "(v)",
                      "token": "v",
                      "kind": "roman_lower",
                      "text": "Finance and business investment;",
                      "subsections": []
                    },
                    {
                      "label": "(vi)",
                      "token": "vi",
                      "kind": "roman_lower",
                      "text": "Innovation; or",
                      "subsections": []
                    },
                    {
                      "label": "(vii)",
                      "token": "vii",
                      "kind": "roman_lower",
                      "text": "Other areas of training or expertise identified by the Governor.",
