                  "label": "(A)",
                  "token": "A",
                  "kind": "alpha_upper",
                  "text": "The period of time covered by the statement.",
                  "subsections": []
                },
                {
                  "label": "(B)",
                  "token": "B",
                  "kind": "alpha_upper",
                  "text": "A statement that all creditors of the estate, other than creditors owed administrative expenses that require court approval, have been paid in full.",
                  "subsections": []
                },
                {
                  "label": "(C)",
                  "token": "C",
                  "kind": "alpha_upper",
                  "text": "The statement and petition and any request for a reserve under subsection",
                  "subsections": []
                }
              ]
            },
            {
              "label": "(c)",
              "token": "c",
              "kind": "alpha_lower",
              "text": "Any request to retain a reserve for the determination and payment of any additional taxes, interest and penalties, and of all related reasonable expenses.",
              "subsections": []
            },
            {
              "label": "(d)",
              "token": "d",
              "kind": "alpha_lower",
              "text": "A statement describing the determination of the compensation of the personal representative under the will or under ORS 113.038 or 116.173",
              "subsections": []
            },
            {
              "label": "(e)",
              "token": "e",
              "kind": "alpha_lower",
              "text": "A petition for a judgment authorizing the personal representative to distribute the estate to the persons and in the portions specified in the judgment.",
              "subsections": []
            },
            {
              "label": "(A)",
              "token": "A",
              "kind": "alpha_upper",
              "text": "The distributees, other than distributees whose only distribution is a cash or specific bequest that will be paid or satisfied in full, consent in writing; and",
              "subsections": []
            },
            {
              "label": "(B)",
              "token": "B",
              "kind": "alpha_upper",
              "text": "All creditors of the estate, other than creditors owed administrative expenses that require court approval, have been paid in full.",
              "subsections": []
            },
            {
              "label": "(D)",
              "token": "D",
              "kind": "alpha_upper",
              "text": "A declaration under penalty of perjury in the form required by ORCP 1 E, or an unsworn declaration under ORS 194.800 to 194.835, if the declarant is physically outside the boundaries of the United States.",
              "subsections": []
            }
