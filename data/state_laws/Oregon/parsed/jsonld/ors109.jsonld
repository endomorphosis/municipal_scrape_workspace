      "identifier": "ORS 109.831",
      "name": "Section 109.831",
      "text": "Application and construction. In applying and construing ORS 109.701 to",
      "history_citation_blocks": [],
      "history_citations": [],
      "subsections": [],
      "parser_warnings": [],
