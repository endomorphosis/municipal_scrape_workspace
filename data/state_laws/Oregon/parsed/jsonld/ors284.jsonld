            },
            {
              "label": "(B)",
              "token": "B",
              "kind": "alpha_upper",
              "text": "One representative recommended by the executive body of the Association of Oregon Counties;",
              "subsections": []
            },
            {
              "label": "(C)",
              "token": "C",
              "kind": "alpha_upper",
              "text": "One representative recommended by the executive body of the League of Oregon Cities;",
              "subsections": []
            },
            {
              "label": "(D)",
              "token": "D",
              "kind": "alpha_upper",
              "text": "One person who represents local and regional business and industry interests; and",
              "subsections": []
            },
            {
              "label": "(E)",
              "token": "E",
              "kind": "alpha_upper",
              "text": "One person who represents philanthropic organizations.",
              "subsections": []
            },
            {
              "label": "(b)",
              "token": "b",
              "kind": "alpha_lower",
              "text": "Regional solutions advisory committees shall: of this subsection.",
              "subsections": [
                {
                  "label": "(A)",
                  "token": "A",
                  "kind": "alpha_upper",
                  "text": "Establish regional priorities for community and economic development in the region;",
                  "subsections": []
                },
                {
                  "label": "(B)",
                  "token": "B",
                  "kind": "alpha_upper",
                  "text": "Assist regional solutions coordinators and regional solutions teams with obtaining, and connecting to, resources and funding; and",
