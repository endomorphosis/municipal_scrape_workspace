      "history_citations": [],
      "subsections": [
        {
          "label": "(1)",
          "token": "1",
          "kind": "numeric",
          "text": "Optometrists utilizing pharmaceutical agents shall be held to the same standards of liability as persons licensed as physicians to practice medicine and surgery by the Oregon Medical Board under ORS chapter 677.",
          "subsections": []
        },
        {
          "label": "(2)",
          "token": "2",
          "kind": "numeric",
          "text": "Notwithstanding ORS 683.010",
