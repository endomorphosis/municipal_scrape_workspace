        },
        {
          "label": "(3)",
          "token": "3",
          "kind": "numeric",
          "text": "The committee shall adopt rules to govern its proceedings and shall select a chair and any other officers it considers necessary.",
          "subsections": []
        },
        {
          "label": "(4)",
          "token": "4",
          "kind": "numeric",
          "text": "The members shall be appointed to serve for terms of three years. A vacancy on the committee shall be filled by appointment by the Public Health Officer for the unexpired term.",
          "subsections": []
        },
        {
          "label": "(5)",
          "token": "5",
          "kind": "numeric",
          "text": "The committee shall meet regularly four times a year at times and places fixed by the chair of the committee. The committee may meet at other times specified by the chair or a majority of the members of the committee.",
          "subsections": []
        },
        {
          "label": "(6)",
          "token": "6",
          "kind": "numeric",
          "text": "The Oregon Health Authority shall provide assistance and space for meetings as requested by the chair of the committee.",
          "subsections": []
        },
        {
          "label": "(7)",
          "token": "7",
          "kind": "numeric",
          "text": "Members of the committee shall be entitled to actual and necessary expenses as provided by ORS 292.495",
          "subsections": []
        }
