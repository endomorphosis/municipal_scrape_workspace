      "name": "Section 276.890",
      "text": "[1975 c.280 19; repealed by 1989 c.97 1] STATE AGENCY FACILITY ENERGY DESIGN",
      "history_citation_blocks": [],
      "history_citations": [],
      "subsections": [],
      "parser_warnings": [],
      "position": 240,
      "isPartOf": "https://www.oregonlegislature.gov/bills_laws/ors/ors276.html"
