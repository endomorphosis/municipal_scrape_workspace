      "@type": "Legislation",
      "@id": "https://www.oregonlegislature.gov/bills_laws/ors/ors677.html#section-677.310",
      "identifier": "ORS 677.310",
      "name": "Section 677.310",
      "text": "[Amended by 1967 c.470 58; repealed by 1989 c.830 49] ENFORCEMENT",
      "history_citation_blocks": [],
      "history_citations": [],
