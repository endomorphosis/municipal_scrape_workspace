      "identifier": "ORS 115.220",
      "name": "Section 115.220",
      "text": "[1963 c.447 1; 1965 c.514 1; repealed by 1969 c.591 305] DISCHARGE OF ENCUMBRANCES",
      "history_citation_blocks": [],
      "history_citations": [],
      "subsections": [],
      "parser_warnings": [],
      "position": 41,
      "isPartOf": "https://www.oregonlegislature.gov/bills_laws/ors/ors115.html"
