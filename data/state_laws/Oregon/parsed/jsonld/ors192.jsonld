            },
            {
              "label": "(b)",
              "token": "b",
              "kind": "alpha_lower",
              "text": "Any individual, including an individual who has died or whose whereabouts are unknown:",
              "subsections": [
                {
                  "label": "(A)",
                  "token": "A",
                  "kind": "alpha_upper",
                  "text": "If the individual by reason of the individuals mental or physical condition or age is unable to authorize such access;",
                  "subsections": []
                },
                {
                  "label": "(B)",
                  "token": "B",
                  "kind": "alpha_upper",
                  "text": "If the individual does not have a legal guardian or other legal representative, or the state or a political subdivision of this state is the legal guardian of the individual; and",
                  "subsections": []
                },
                {
                  "label": "(C)",
                  "token": "C",
                  "kind": "alpha_upper",
                  "text": "If a complaint regarding the rights or safety of the individual has been received by the system or if, as a result of monitoring or other activities which result from a complaint or other evidence, there is probable cause to believe that the individual has been subject to abuse or neglect; and",
                  "subsections": []
                }
              ]
            },
            {
              "label": "(c)",
              "token": "c",
              "kind": "alpha_lower",
              "text": "Any individual who has a legal guardian or other legal representative, who is the subject of a complaint of abuse or neglect received by the system, or whose health and safety is believed with probable cause to be in serious and immediate jeopardy if the legal guardian or other legal representative:",
              "subsections": [
                {
                  "label": "(A)",
                  "token": "A",
                  "kind": "alpha_upper",
                  "text": "Has been contacted by the system upon receipt of the name and address of the legal guardian or other legal representative;",
                  "subsections": []
                },
                {
                  "label": "(B)",
                  "token": "B",
                  "kind": "alpha_upper",
                  "text": "Has been offered assistance by the system to resolve the situation; and",
                  "subsections": []
                },
                {
                  "label": "(C)",
                  "token": "C",
                  "kind": "alpha_upper",
                  "text": "Has failed or refused to act on behalf of the individual.",
                  "subsections": []
                }
              ]
            }
          ]
        },
        {
          "label": "(2)",
          "token": "2",
          "kind": "numeric",
          "text": "The system shall have access to the name, address and telephone number of any legal guardian or other legal representative of an individual.",
          "subsections": []
        },
        {
          "label": "(3)",
          "token": "3",
          "kind": "numeric",
          "text": "The system that obtains access to records under this section shall maintain the confidentiality of the records to the same extent as is required of the provider of the services, except as provided under the Protection and Advocacy for Mentally Ill Individuals Act (42 U.S.C. 10806) as in effect on January 1, 2003.",
          "subsections": []
        },
        {
          "label": "(4)",
          "token": "4",
          "kind": "numeric",
          "text": "The system shall have reasonable access to facilities, including the residents and staff of the facilities.",
          "subsections": []
        },
        {
          "label": "(5)",
          "token": "5",
          "kind": "numeric",
          "text": "This section is not intended to limit or overrule the provisions of ORS 41.675 or 441.055",
          "subsections": []
        },
        {
          "label": "(7)",
          "token": "7",
          "kind": "numeric",
          "text": ". [1993 c.262 2; 1995 c.504 2; 2003 c.14 93; 2003 c.803 8; 2005 c.498 8; 2009 c.595 165; 2009 c.792 72] Note: See note under 192.515.",
          "subsections": []
        }
      ],
