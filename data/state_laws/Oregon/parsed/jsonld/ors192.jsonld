      "identifier": "ORS 192.530",
      "name": "Section 192.530",
      "text": "[1977 c.812 2; 1995 c.79 71; repealed by 2003 c.86 8] GENETIC PRIVACY",
      "history_citation_blocks": [],
      "history_citations": [],
      "subsections": [],
      "parser_warnings": [],
      "position": 131,
      "isPartOf": "https://www.oregonlegislature.gov/bills_laws/ors/ors192.html"
